# Gemini API Key
# Get your API key from: https://aistudio.google.com/apikey
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# Payment Integration
# ЮKassa (для России)
VITE_YUKASSA_SHOP_ID=your_yukassa_shop_id
VITE_YUKASSA_SECRET_KEY=your_yukassa_secret_key

# Stripe (для международных платежей)
VITE_STRIPE_PUBLISHABLE_KEY=pk_live_xxxxxxxxxxxxx
VITE_STRIPE_SECRET_KEY=sk_live_xxxxxxxxxxxxx

# Analytics
VITE_GA_MEASUREMENT_ID=G-XXXXXXXXXX
VITE_YM_COUNTER_ID=98765432

# Application Settings
VITE_APP_URL=https://primerochnaya.vercel.app
VITE_API_URL=https://api.primerochnaya.vercel.app

# Feature Flags
VITE_ENABLE_PAYMENTS=true
VITE_ENABLE_B2B=true
VITE_ENABLE_API=false
